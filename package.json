{
  "name": "vue-qb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve-bundle": "vue-cli-service serve ./src/components/index.js",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-bundle": "npm run build-bundle-vue-qb && npm run build-bundle-vue-qb-bs3",
    "build-bundle-vue-qb": "vue-cli-service build --target lib --dest dist/vue-qb --name vue-qb ./src/components/index.js",
    "build-bundle-vue-qb-bs3": "vue-cli-service build --target lib --dest dist/vue-qb-bs3 --name vue-qb-bs3 ./src/components/index-bs3.js"
  },
  "dependencies": {
    "vue": "^2.6.11",
    "vue-deepset": "^0.6.3",
    "vue-router": "^3.1.6",
    "vue-uuid": "^1.1.1",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-service": "^5.0.8",
    "acorn": "^7.1.1",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^4.4.1",
    "bootstrap3": "npm:bootstrap@^3.4.1",
    "core-js": "^2.6.11",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "eslint-utils": ">=1.4.1",
    "expose-loader": "^0.7.5",
    "jquery": "^3.5.0",
    "kind-of": ">=6.0.3",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "minimist": ">=1.2.3",
    "node-sass": "^4.13.1",
    "popper.js": "^1.16.1",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.3",
    "sass-loader": "^7.3.1",
    "serialize-javascript": "^3.1.0",
    "style-loader": "^0.23.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
